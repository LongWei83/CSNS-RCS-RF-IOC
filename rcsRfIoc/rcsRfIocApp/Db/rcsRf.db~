record(waveform,"$(IOC):wf_1") {
        field(SCAN,"I/O Intr")
        field(DTYP,"CPCI9110 READ")
        field(INP,"#C$(Card) S @WF_1")
        field(NELM,"2048")
        field(FTVL,"FLOAT")
}

