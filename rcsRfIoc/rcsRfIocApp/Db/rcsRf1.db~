record(bi,"rcsRf1:intEnable_get") {
        field(DESC, "")
        field(DTYP,"D212")
        field(INP,"#C0 S @INT_ENABLE")
        field(ZNAM,"0")
        field(ONAM,"1")
        field(SCAN,".1 second")
}

record(bi,"rcsRf1:sweepOption_get") {
        field(DESC, "")
        field(DTYP,"D212")
        field(INP,"#C0 S @SWEEP_OPTION")
        field(ZNAM,"0")
        field(ONAM,"1")
        field(SCAN,".1 second")
}

record(bo,"rcsRf1:intEnable_set") {
        field(DESC, "")
        field(DTYP,"D212")
        field(OUT,"#C0 S @INT_ENABLE")
        field(ZNAM,"0")
        field(ONAM,"1")
}

record(bo,"rcsRf1:sweepOption_set") {
        field(DESC, "")
        field(DTYP,"D212")
        field(OUT,"#C0 S @SWEEP_OPTION")
        field(ZNAM,"0")
        field(ONAM,"1")
}

record(ai,"rcsRf1:workPeriod_get")
{
        field(DESC, "")
        field(PREC, "4")
        field(DTYP, "D212")
        field(INP, "#C0 S @WORK_PERIOD")
        field(SCAN,".1 second")
	field(EGU,"ms")
}

record(ao,"rcsRf1:workPeriod_set")
{
        field(DESC, "")
        field(PREC, "4")
        field(DTYP, "D212")
        field(OUT, "#C0 S @WORK_PERIOD")
}

record(waveform,"rcsRf1:wf_3") {
        field(SCAN,"I/O Intr")
        field(DTYP,"D212 READ")
        field(INP,"#C0 S @WF_3")
        field(NELM,"2048")
        field(FTVL,"FLOAT")
}
